{
  "name": "dapeng-redux",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "build:local": "cross-env MODE=dev nuxt build && npm start",
    "dev": "cross-env MODE=dev nuxt",
    "start": "cross-env MODE=dev nuxt start",
    "test": "cross-env MODE=test nuxt build && ENABLE_NODE_LOG=YES pm2 start --only M-5.1-test",
    "dev:spa": "cross-env MODE=dev nuxt build  --spa",
    "test:spa": "cross-env MODE=test nuxt build  --spa",
    "pro:spa": "cross-env MODE=pro nuxt build  --spa",
    "pro": "cross-env MODE=pro nuxt build && ENABLE_NODE_LOG=YES pm2 start --only M-5.1-pro",
    "build:test": "cross-env MODE=test nuxt build && pm2 restart all",
    "build": "cross-env MODE=pro nuxt build && pm2 restart all",
    "an": "cross-env MODE=dev nuxt build --analyze"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "nuxt": {
      "port": "3000",
      "host": "127.0.0.1"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@vant/touch-emulator": "^1.2.0",
    "ali-oss": "^6.11.2",
    "amfe-flexible": "^2.2.1",
    "animate.css": "^4.1.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.36",
    "image-size": "^0.9.3",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.0.0",
    "nuxt": "^2.14.5",
    "root-font-size": "^1.0.4",
    "swiper": "^5.4.5",
    "vant": "^2.10.14",
    "vue-awesome-swiper": "^4.1.1",
    "vue-clipboard2": "^0.3.1",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "postcss-pxtorem": "^5.1.1"
  }
}
