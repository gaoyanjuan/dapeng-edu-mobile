{
  "name": "dapeng-redux",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "build:local": "cross-env MODE=dev nuxt build && npm start",
    "dev": "cross-env MODE=dev nuxt",
    "start": "cross-env MODE=dev nuxt start",
    "dev:spa": "cross-env MODE=dev nuxt build  --spa",
    "test:spa": "cross-env MODE=test nuxt build  --spa",
    "pro:spa": "cross-env MODE=pro nuxt build  --spa",
    "build:test": "cross-env MODE=test nuxt build",
    "build:pro": "cross-env MODE=pro nuxt build",
    "pm2:test": "ENABLE_NODE_LOG=YES pm2 start --only wap-test",
    "pm2:pro": "ENABLE_NODE_LOG=YES pm2 start --only wap-pro",
    "pm2:restart": "pm2 restart all"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
