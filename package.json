    "amfe-flexible": "^2.2.1",
    "animate.css": "^4.1.1",
    "btoa": "^1.2.1",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.36",
    "html2canvas": "^1.0.0-rc.7",
    "image-size": "^0.9.3",
    "jwt-decode": "^3.1.2",
    "nuxt": "^2.14.5",
    "nuxt-matomo": "^1.2.3",
    "root-font-size": "^1.0.4",
    "swiper": "^5.4.5",
    "vant": "^2.11.0",
    "vconsole": "^3.4.0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-clipboard2": "^0.3.1",
    "xss": "^1.0.8"
  },
